class ComponentOne extends React.Component 
			{
				render()
				{
					return (

						<div>
						<h1>{this.props.text}</h1>
						<p>{this.props.children}</p>
						</div>
					)
				}
			}

		ReactDOM.render
		(
			<div>
				<ComponentOne text="Hello World!">Working</ComponentOne>
				<ComponentOne text="Hello World!">Working</ComponentOne>
				<ComponentOne text="Hello World!">Working</ComponentOne>
			</div>,
			document.getElementById('react-container')				
		)